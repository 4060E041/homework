#include <iostream>

using namespace std;

int main()
{
 
string s1("Welcome");
s1.append(" to CPP"); 
cout << s1 << endl;
// 將字串附加於字串物件後 
string s2("Welcome");
s2.append(" to C and Cpp", 3, 6); 
cout << s2 << endl;
//從字串的index位置開始n到n個字元前,附加於字串上 
string s3("Welcome");
s3.append(" to C and Cpp", 7); 
cout << s3 << endl; 
//從字串的index位置n個字元前,附加於字串上 
string s4("Welcome"); 
s4.append(14, 'F'); 
cout << s4 << endl; 
//將字元拷貝n次,附加於字串後 
  return 0;
}
