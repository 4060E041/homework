#include <iostream>

using namespace std;

int main()
{
 
string s1("Hello");
s1.append("GiannisAntetokounmpo"); 
cout << s1 << endl;
// 將字串附加於字串物件後 
string s2("Hello");
s2.append("GiannisAntetokounmpo", 3, 6); 
cout << s2 << endl;
//從字串的index位置開始n到n個字元前,附加於字串上 
string s3("Hello");
s3.append("GiannisAntetokounmpo", 7); 
cout << s3 << endl; 
//從字串的index位置n個字元前,附加於字串上 
string s4("Hello"); 
s4.append(5, '5'); 
cout << s4 << endl; 
//將字元拷貝n次,附加於字串後 
  return 0;
}
/*HelloGiannisAntetokounmpo
HellonnisAn
HelloGiannis
Hello55555

--------------------------------
Process exited after 0.01633 seconds with return value 0
請按任意鍵繼續 . . .*/








